{
  "$schema": "http://json.schemastore.org/template",
  "author": "Casper Rub√¶k",
  "classifications": [ "Rest API Project", "API Controller" ],
  "identity": "AppBlueprint.Template.ApiControllerProject",
  "name": "AppBlueprint API Controller",
  "shortName": "appblueprint-api-controller-project",
  "tags": {
    "language": "C#",
